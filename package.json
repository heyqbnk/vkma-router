{
  "name": "vkma-router",
  "version": "1.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:wolframdeus/vkma-router.git",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "webpack-dev-server --https",
    "test": "tsc --noEmit",
    "build": "gulp",
    "lint": "eslint src/**/*.{ts,tsx}",
    "validate": "npm-run-all lint test",
    "commit": "git-cz",
    "prerelease": "npm-run-all validate build",
    "release": "standard-version -a",
    "release:tags": "git push --follow-tags origin master && yarn publish --non-interactive",
    "release:github": "dotenv -e .env -- conventional-github-releaser -p eslint",
    "postrelease": "npm-run-all release:*"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/history": "^4.7.5",
    "@types/qs": "^6.9.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-minify": "^3.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "react-dom": "^16.13.1",
    "standard-version": "^8.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "history": "^4.10.1",
    "qs": "^6.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "@vkontakte/vk-bridge": "^2.2.1",
    "react": "^16.13.1"
  }
}
